@keyframes pulse-animation
    0%
        transform scale(1)

    30%
        transform scale(1)

    40%
        transform scale(1.25)

    50%
        transform scale(1)

    60%
        transform scale(1)

    70%
        transform scale(1.4)

    80%
        transform scale(1)

    100%
        transform scale(1)
